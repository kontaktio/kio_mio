<%= form_with(model: telemetry) do |form| %>
  <% if telemetry.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(telemetry.errors.count, "error") %> prohibited this telemetry from being saved:</h2>

      <ul>
        <% telemetry.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :tracking_id, style: "display: block" %>
    <%= form.text_field :tracking_id %>
  </div>

  <div>
    <%= form.label :timestamp, style: "display: block" %>
    <%= form.text_field :timestamp %>
  </div>

  <div>
    <%= form.label :battery_level, style: "display: block" %>
    <%= form.number_field :battery_level %>
  </div>

  <div>
    <%= form.label :channel, style: "display: block" %>
    <%= form.number_field :channel %>
  </div>

  <div>
    <%= form.label :humidity, style: "display: block" %>
    <%= form.number_field :humidity %>
  </div>

  <div>
    <%= form.label :light, style: "display: block" %>
    <%= form.number_field :light %>
  </div>

  <div>
    <%= form.label :rssi, style: "display: block" %>
    <%= form.number_field :rssi %>
  </div>

  <div>
    <%= form.label :temperature, style: "display: block" %>
    <%= form.text_field :temperature %>
  </div>

  <div>
    <%= form.label :air_quality, style: "display: block" %>
    <%= form.number_field :air_quality %>
  </div>

  <div>
    <%= form.label :firmware, style: "display: block" %>
    <%= form.text_field :firmware %>
  </div>

  <div>
    <%= form.label :model, style: "display: block" %>
    <%= form.number_field :model %>
  </div>

  <div>
    <%= form.label :moving, style: "display: block" %>
    <%= form.check_box :moving %>
  </div>

  <div>
    <%= form.label :client_id, style: "display: block" %>
    <%= form.number_field :client_id %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
