<div id="<%= dom_id room %>">
  <p>
    <strong>Client:</strong>
    <%= link_to room.client.name, room.client %>
  </p>
  <p>
    <strong>Room:</strong>
    <%= room.room_id %>
  </p>
  <p>
    <strong>Name:</strong>
    <%= room.name %>
  </p>
  <p>
    <strong>Floor name:</strong>
    <%= room.floor_name %>
  </p>
  <p>
    <strong>Building name:</strong>
    <%= room.building_name %>
  </p>
</div>
